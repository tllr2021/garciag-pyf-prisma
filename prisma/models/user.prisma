enum UserType {
  ADMIN
  EMPLOYEE
  CLIENT
}

type User {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  email: String! @unique
  password: String!
  birthday: DateTime
  name: String!
  isMember: Boolean! @default(value: false)
  type: UserType!
  card: Card @relation(link: INLINE)
}
