enum Rating {
  G
  PG
  PG13
  R
  NC17
}

enum Genre {
  HORROR
  COMEDY
  ACTION
  SCIFI
  SUSPENSE
  ANIMATION
  THRILLER
  WESTERN
}

type Movie {
  id: ID! @id
  title: String!
  director: String!
  description: String!
  length: Int!
  rating: Rating!
  genre: [Genre!]! @relation(link: INLINE)
}